window.dataLayer = window.dataLayer || [];
function gtag() {
  // eslint-disable-next-line no-undef,prefer-rest-params
  dataLayer.push(arguments);
}
gtag('js', new Date());

gtag('config', 'G-4Y3WKF2MY1', { optimize_id: 'GTM-PFXPCSV' });

